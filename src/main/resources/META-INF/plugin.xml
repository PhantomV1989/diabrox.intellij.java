<idea-plugin>
    <id>com.github.phantomv1989.diabrox</id>
    <name>diabrox</name>
    <vendor>phantomv1989</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.github.phantomv1989.diabrox.services.MyApplicationService"/>
        <projectService serviceImplementation="com.github.phantomv1989.diabrox.services.MyProjectService"/>
    </extensions>

    <applicationListeners>
        <listener class="com.github.phantomv1989.diabrox.listeners.MyProjectManagerListener"
                  topic="com.intellij.openapi.project.ProjectManagerListener"/>
    </applicationListeners>
    <actions>
        <action id="com.github.phantomv1989.diabrox.actions.ActionA"
                class="com.github.phantomv1989.diabrox.actions.ActionA" text="actionA"/>
    </actions>
</idea-plugin>
